а)
#include <iostream>
using namespace std;
int main()
{
    float y, x;
  cout <<"Enter variable value"; 
  cin >> x; //Вводим значение переменной
  y = 7 * x * x - 3 * x + 6; // Задаем формулу вычисления значения функции 
  cout << "function value is " << y << "\n"; //Ответ
        cin.get();
  return 0;
}



б)
#include <iostream>
using namespace std;
int main()
{
    float a, x;
    cout <<"Enter variable value";
    cin >> a; //Вводим значение переменной
    x = 12 * a * a + 7 * a - 16 ; // Задаем формулу вычисления значения функции
    cout << "function value is " << x << "\n"; //Ответ
    cin.get();
    return 0;
}
